{
  "expo": {
    "name": "parentsApp",
    "slug": "parentsApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "parentsapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "assets/images/**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "notification": {
        "icon": "./assets/images/favicon/notification.png",
        "color": "#E91E63"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.professorpcthomas.com.parentsApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      [
        "expo-splash-screen",
       {
          "image": "./assets/images/splashScreen/transparent.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "BASEURL": "https://backend.professorpcthomas.com/api",
      "eas": {
        "projectId": "96aaa33f-5856-4937-a0e4-c9bc0b71ac16"
      }
    },
    "owner": "professorpcthomas.com"
  }
}
